generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

enum ClientType {
  INDIVIDUAL
  LEGAL_ENTITY
}

model Bank {
  id   Int    @id @default(autoincrement())
  name String @unique
}

model Client {
  id   Int        @id @default(autoincrement())
  name String
  type ClientType @default(INDIVIDUAL)
}
